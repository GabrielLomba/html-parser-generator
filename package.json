{
  "name": "html-parser-generator",
  "version": "1.0.0",
  "description": "A microservice for generating HTML parsers using OpenAI",
  "type": "module",
  "main": "dist/index.js",
      "scripts": {
        "build": "tsup",
        "build:watch": "tsup --watch",
        "start": "node dist/index.js",
        "dev": "tsup --watch & nodemon dist/index.js",
        "test": "tsup && node dist/tests/runTests.js",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write src/**/*.ts",
        "format:check": "prettier --check src/**/*.ts",
        "prepare": "husky"
    },
  "keywords": [
    "html",
    "parser",
    "openai",
    "microservice"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "openai": "^4.20.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.1",
    "tsup": "^8.5.0",
    "typescript": "^5.2.2"
  }
}
